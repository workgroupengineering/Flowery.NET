<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:controls="clr-namespace:Flowery.Controls;assembly=Flowery.NET"
             xmlns:local="clr-namespace:Flowery.NET.Gallery.Examples"
             x:Class="Flowery.NET.Gallery.Examples.NavigationExamples">

    <Grid>
        <ScrollViewer x:Name="MainScrollViewer" HorizontalScrollBarVisibility="Disabled">
            <StackPanel Spacing="24" Margin="24,0,24,32">

                <!-- Breadcrumbs -->
                <StackPanel Spacing="12">
                    <local:SectionHeader SectionId="breadcrumbs" Title="Breadcrumbs" />
                    <WrapPanel Orientation="Horizontal">
                        <!-- Basic breadcrumbs -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Basic" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyBreadcrumbs>
                                <controls:DaisyBreadcrumbItem Content="Home" />
                                <controls:DaisyBreadcrumbItem Content="Documents" />
                                <controls:DaisyBreadcrumbItem Content="Add Document" />
                            </controls:DaisyBreadcrumbs>
                        </StackPanel>

                        <!-- With custom separator -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Custom Separator (›)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyBreadcrumbs Separator="›">
                                <controls:DaisyBreadcrumbItem Content="Home" />
                                <controls:DaisyBreadcrumbItem Content="Products" />
                                <controls:DaisyBreadcrumbItem Content="Details" />
                            </controls:DaisyBreadcrumbs>
                        </StackPanel>

                        <!-- With chevron separator -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Chevron Separator (»)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyBreadcrumbs Separator="»" SeparatorOpacity="0.7">
                                <controls:DaisyBreadcrumbItem Content="Root" />
                                <controls:DaisyBreadcrumbItem Content="Folder" />
                                <controls:DaisyBreadcrumbItem Content="File" />
                            </controls:DaisyBreadcrumbs>
                        </StackPanel>

                        <!-- With icons -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="With Icons" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyBreadcrumbs>
                                <controls:DaisyBreadcrumbItem Content="Home" Icon="{StaticResource DaisyIconHome}" />
                                <controls:DaisyBreadcrumbItem Content="Folder" Icon="{StaticResource DaisyIconFolder}" />
                                <controls:DaisyBreadcrumbItem Content="File" Icon="{StaticResource DaisyIconDocument}" />
                            </controls:DaisyBreadcrumbs>
                        </StackPanel>

                        <!-- Max-width with scrolling -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="With Max-Width (scrollable)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyBreadcrumbs MaxWidth="220" MinHeight="80">
                                <controls:DaisyBreadcrumbItem Content="Long text 1" />
                                <controls:DaisyBreadcrumbItem Content="Long text 2" />
                                <controls:DaisyBreadcrumbItem Content="Long text 3" />
                                <controls:DaisyBreadcrumbItem Content="Long text 4" />
                                <controls:DaisyBreadcrumbItem Content="Long text 5" />
                            </controls:DaisyBreadcrumbs>
                        </StackPanel>
                    </WrapPanel>
                </StackPanel>

                <controls:DaisyDivider />

                <!-- Menu -->
                <StackPanel Spacing="12">
                    <local:SectionHeader SectionId="menu" Title="Menu" />
                    <TextBlock Text="Menu is used to display a list of links vertically or horizontally" Opacity="0.7" FontSize="13"/>

                    <!-- Basic Menu -->
                    <TextBlock Text="Basic Menu" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Vertical (default)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="200" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Item 1</ListBoxItem>
                                <ListBoxItem>Item 2</ListBoxItem>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Horizontal" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Orientation="Horizontal" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Item 1</ListBoxItem>
                                <ListBoxItem>Item 2</ListBoxItem>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Menu with Icons Only -->
                    <TextBlock Text="Menu with Icon Only" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Vertical" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>
                                    <PathIcon Data="{StaticResource DaisyIconHome}" Width="20" Height="20"/>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <PathIcon Data="{StaticResource DaisyIconInfo}" Width="20" Height="20"/>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <PathIcon Data="{StaticResource DaisyIconChart}" Width="20" Height="20"/>
                                </ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Horizontal" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Orientation="Horizontal" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>
                                    <PathIcon Data="{StaticResource DaisyIconHome}" Width="20" Height="20"/>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <PathIcon Data="{StaticResource DaisyIconInfo}" Width="20" Height="20"/>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <PathIcon Data="{StaticResource DaisyIconChart}" Width="20" Height="20"/>
                                </ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Menu Sizes -->
                    <TextBlock Text="Menu Sizes" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Extra Small" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="180" Size="ExtraSmall" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Xsmall 1</ListBoxItem>
                                <ListBoxItem>Xsmall 2</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Small" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="180" Size="Small" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Small 1</ListBoxItem>
                                <ListBoxItem>Small 2</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Medium (default)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="180" Size="Medium" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Medium 1</ListBoxItem>
                                <ListBoxItem>Medium 2</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Large" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="180" Size="Large" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Large 1</ListBoxItem>
                                <ListBoxItem>Large 2</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Extra Large" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="180" Size="ExtraLarge" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Xlarge 1</ListBoxItem>
                                <ListBoxItem>Xlarge 2</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Menu with Disabled Items -->
                    <TextBlock Text="Menu with Disabled Items" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <controls:DaisyMenu Width="200" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Enabled item</ListBoxItem>
                                <ListBoxItem IsEnabled="False">Disabled item</ListBoxItem>
                                <ListBoxItem IsEnabled="False">Disabled item</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Menu with Icons -->
                    <TextBlock Text="Menu with Icons" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Vertical with icons" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="200" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconHome}" Width="16" Height="16"/>
                                        <TextBlock Text="Home" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconInfo}" Width="16" Height="16"/>
                                        <TextBlock Text="Details" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconChart}" Width="16" Height="16"/>
                                        <TextBlock Text="Stats" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="With icons and badge" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Orientation="Horizontal" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconInbox}" Width="16" Height="16"/>
                                        <TextBlock Text="Inbox" VerticalAlignment="Center"/>
                                        <controls:DaisyBadge Content="99+" Size="Small"/>
                                    </StackPanel>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconInfo}" Width="16" Height="16"/>
                                        <TextBlock Text="Updates" VerticalAlignment="Center"/>
                                        <controls:DaisyBadge Content="NEW" Size="ExtraSmall" Variant="Warning"/>
                                    </StackPanel>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <TextBlock Text="Stats" VerticalAlignment="Center"/>
                                        <controls:DaisyBadge Size="ExtraSmall" Variant="Info"/>
                                    </StackPanel>
                                </ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Menu with Title -->
                    <TextBlock Text="Menu with Title" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Title as list item" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="200" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem Classes="menu-title">Title</ListBoxItem>
                                <ListBoxItem>Item 1</ListBoxItem>
                                <ListBoxItem>Item 2</ListBoxItem>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Menu with Active Item -->
                    <TextBlock Text="Menu with Active Item" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Default active colors" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="200" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Item 1</ListBoxItem>
                                <ListBoxItem IsSelected="True">Selected Item</ListBoxItem>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Custom active colors (Primary)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="200" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8"
                                               ActiveForeground="{DynamicResource DaisyPrimaryContentColor}"
                                               ActiveBackground="{DynamicResource DaisyPrimaryBrush}">
                                <ListBoxItem>Item 1</ListBoxItem>
                                <ListBoxItem IsSelected="True">Selected Item</ListBoxItem>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Accent active colors" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Orientation="Horizontal" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8"
                                               ActiveForeground="{DynamicResource DaisyAccentContentColor}"
                                               ActiveBackground="{DynamicResource DaisyAccentBrush}">
                                <ListBoxItem>Home</ListBoxItem>
                                <ListBoxItem IsSelected="True">About</ListBoxItem>
                                <ListBoxItem>Contact</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Using menu-active class" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="200" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Item 1</ListBoxItem>
                                <ListBoxItem Classes="menu-active">Active Item</ListBoxItem>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Collapsible Submenu -->
                    <TextBlock Text="Collapsible Submenu" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Using Expander" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Width="220" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Item 1</ListBoxItem>
                                <Expander Header="Parent" IsExpanded="True">
                                    <controls:DaisyMenu>
                                        <ListBoxItem>Submenu 1</ListBoxItem>
                                        <ListBoxItem>Submenu 2</ListBoxItem>
                                        <Expander Header="Nested Parent" IsExpanded="True">
                                            <controls:DaisyMenu>
                                                <ListBoxItem>Nested 1</ListBoxItem>
                                                <ListBoxItem>Nested 2</ListBoxItem>
                                            </controls:DaisyMenu>
                                        </Expander>
                                    </controls:DaisyMenu>
                                </Expander>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- File Tree Example -->
                    <TextBlock Text="File Tree Example" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <controls:DaisyMenu Width="260" Size="ExtraSmall" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconDocument}" Width="14" Height="14"/>
                                        <TextBlock Text="resume.pdf" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                                <Expander IsExpanded="True">
                                    <Expander.Header>
                                        <StackPanel Orientation="Horizontal" Spacing="8">
                                            <PathIcon Data="{StaticResource DaisyIconFolder}" Width="14" Height="14"/>
                                            <TextBlock Text="My Files" VerticalAlignment="Center"/>
                                        </StackPanel>
                                    </Expander.Header>
                                    <controls:DaisyMenu Size="ExtraSmall">
                                        <ListBoxItem>
                                            <StackPanel Orientation="Horizontal" Spacing="8">
                                                <PathIcon Data="{StaticResource DaisyIconDocument}" Width="14" Height="14"/>
                                                <TextBlock Text="Project-final.psd" VerticalAlignment="Center"/>
                                            </StackPanel>
                                        </ListBoxItem>
                                        <Expander IsExpanded="True">
                                            <Expander.Header>
                                                <StackPanel Orientation="Horizontal" Spacing="8">
                                                    <PathIcon Data="{StaticResource DaisyIconFolder}" Width="14" Height="14"/>
                                                    <TextBlock Text="Images" VerticalAlignment="Center"/>
                                                </StackPanel>
                                            </Expander.Header>
                                            <controls:DaisyMenu Size="ExtraSmall">
                                                <ListBoxItem>
                                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                                        <PathIcon Data="{StaticResource DaisyIconImage}" Width="14" Height="14"/>
                                                        <TextBlock Text="Screenshot1.png" VerticalAlignment="Center"/>
                                                    </StackPanel>
                                                </ListBoxItem>
                                                <ListBoxItem>
                                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                                        <PathIcon Data="{StaticResource DaisyIconImage}" Width="14" Height="14"/>
                                                        <TextBlock Text="Screenshot2.png" VerticalAlignment="Center"/>
                                                    </StackPanel>
                                                </ListBoxItem>
                                            </controls:DaisyMenu>
                                        </Expander>
                                    </controls:DaisyMenu>
                                </Expander>
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconDocument}" Width="14" Height="14"/>
                                        <TextBlock Text="reports-final.pdf" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Horizontal Menu -->
                    <TextBlock Text="Horizontal Menu" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Basic horizontal" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Orientation="Horizontal" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>Item 1</ListBoxItem>
                                <ListBoxItem>Item 2</ListBoxItem>
                                <ListBoxItem>Item 3</ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="With icons" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyMenu Orientation="Horizontal" Background="{DynamicResource DaisyBase200Brush}" CornerRadius="16" Padding="8">
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconHome}" Width="16" Height="16"/>
                                        <TextBlock Text="Home" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconInfo}" Width="16" Height="16"/>
                                        <TextBlock Text="About" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                                <ListBoxItem>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <PathIcon Data="{StaticResource DaisyIconSettings}" Width="16" Height="16"/>
                                        <TextBlock Text="Settings" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </ListBoxItem>
                            </controls:DaisyMenu>
                        </StackPanel>
                    </WrapPanel>
                </StackPanel>

                <controls:DaisyDivider />

                <!-- Navbar -->
                <StackPanel Spacing="12">
                    <local:SectionHeader SectionId="navbar" Title="Navbar" />
                    <TextBlock Text="Navbar is used to show a navigation bar on the top of the page. Use NavbarStart, NavbarCenter, and NavbarEnd properties for layout." Opacity="0.7" FontSize="13"/>

                    <!-- Basic Navbars -->
                    <TextBlock Text="Basic Navbars" FontWeight="SemiBold" Margin="0,8,0,0"/>
                    <StackPanel Spacing="16">
                        <!-- Navbar with title only -->
                        <StackPanel Spacing="6">
                            <TextBlock Text="Title only" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                            </controls:DaisyNavbar>
                        </StackPanel>

                        <!-- Navbar with title and icon -->
                        <StackPanel Spacing="6">
                            <TextBlock Text="Title and icon" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <controls:DaisyButton Variant="Ghost" Shape="Circle">
                                        <PathIcon Data="{StaticResource DaisyIconEllipsis}" Width="20" Height="20"/>
                                    </controls:DaisyButton>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>

                        <!-- Navbar with icons at start and end -->
                        <StackPanel Spacing="6">
                            <TextBlock Text="Icons at start and end" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <controls:DaisyButton Variant="Ghost" Shape="Circle">
                                            <PathIcon Data="{StaticResource DaisyIconMenu}" Width="20" Height="20"/>
                                        </controls:DaisyButton>
                                        <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                    </StackPanel>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <controls:DaisyButton Variant="Ghost" Shape="Circle">
                                        <PathIcon Data="{StaticResource DaisyIconEllipsis}" Width="20" Height="20"/>
                                    </controls:DaisyButton>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Navbar with Menu -->
                    <TextBlock Text="Navbar with Menu" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <!-- Navbar with horizontal menu -->
                        <StackPanel Spacing="6">
                            <TextBlock Text="With horizontal menu" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <controls:DaisyMenu Orientation="Horizontal">
                                        <ListBoxItem>Home</ListBoxItem>
                                        <ListBoxItem>About</ListBoxItem>
                                        <ListBoxItem>Contact</ListBoxItem>
                                    </controls:DaisyMenu>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>

                        <!-- Navbar with center menu -->
                        <StackPanel Spacing="6">
                            <TextBlock Text="Center menu with action button" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarCenter>
                                    <controls:DaisyMenu Orientation="Horizontal">
                                        <ListBoxItem>Item 1</ListBoxItem>
                                        <ListBoxItem>Item 2</ListBoxItem>
                                        <ListBoxItem>Item 3</ListBoxItem>
                                    </controls:DaisyMenu>
                                </controls:DaisyNavbar.NavbarCenter>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <controls:DaisyButton Variant="Primary" Content="Get Started"/>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Navbar with Search and Avatar -->
                    <TextBlock Text="Navbar with Search and Avatar" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <StackPanel Spacing="6">
                            <TextBlock Text="Search input and avatar" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <controls:DaisyInput Watermark="Search..." Width="150" Size="Small"/>
                                        <controls:DaisyAvatar Size="Small" Shape="Circle">
                                            <PathIcon Data="{StaticResource DaisyIconUser}" Width="20" Height="20"/>
                                        </controls:DaisyAvatar>
                                    </StackPanel>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Navbar with Dropdown, Center Logo, and Icons -->
                    <TextBlock Text="Center Logo with Side Icons" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <StackPanel Spacing="6">
                            <TextBlock Text="Menu button, center title, action icons" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" Shape="Circle">
                                        <PathIcon Data="{StaticResource DaisyIconMenu}" Width="20" Height="20"/>
                                    </controls:DaisyButton>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarCenter>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarCenter>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <StackPanel Orientation="Horizontal" Spacing="4">
                                        <controls:DaisyButton Variant="Ghost" Shape="Circle">
                                            <PathIcon Data="{StaticResource DaisyIconSearch}" Width="20" Height="20"/>
                                        </controls:DaisyButton>
                                        <controls:DaisyIndicator>
                                            <controls:DaisyIndicator.Badge>
                                                <controls:DaisyBadge Variant="Primary" Size="ExtraSmall"/>
                                            </controls:DaisyIndicator.Badge>
                                            <controls:DaisyButton Variant="Ghost" Shape="Circle">
                                                <PathIcon Data="{StaticResource DaisyIconBell}" Width="20" Height="20"/>
                                            </controls:DaisyButton>
                                        </controls:DaisyIndicator>
                                    </StackPanel>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Navbar with Indicator -->
                    <TextBlock Text="Navbar with Indicator" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <StackPanel Spacing="6">
                            <TextBlock Text="Shopping cart with item count" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <StackPanel Orientation="Horizontal" Spacing="8">
                                        <controls:DaisyIndicator>
                                            <controls:DaisyIndicator.Badge>
                                                <controls:DaisyBadge Content="8" Size="Small"/>
                                            </controls:DaisyIndicator.Badge>
                                            <controls:DaisyButton Variant="Ghost" Shape="Circle">
                                                <PathIcon Data="{StaticResource DaisyIconShoppingCart}" Width="20" Height="20"/>
                                            </controls:DaisyButton>
                                        </controls:DaisyIndicator>
                                        <controls:DaisyAvatar Size="Small" Shape="Circle">
                                            <PathIcon Data="{StaticResource DaisyIconUser}" Width="20" Height="20"/>
                                        </controls:DaisyAvatar>
                                    </StackPanel>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Navbar with Colors -->
                    <TextBlock Text="Navbar with Colors" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <StackPanel Spacing="6">
                            <TextBlock Text="Neutral background" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar Background="{DynamicResource DaisyNeutralBrush}" Foreground="{DynamicResource DaisyNeutralContentBrush}">
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                            </controls:DaisyNavbar>
                        </StackPanel>

                        <StackPanel Spacing="6">
                            <TextBlock Text="Base-300 background" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar Background="{DynamicResource DaisyBase300Brush}">
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                            </controls:DaisyNavbar>
                        </StackPanel>

                        <StackPanel Spacing="6">
                            <TextBlock Text="Primary background" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar Background="{DynamicResource DaisyPrimaryBrush}" Foreground="{DynamicResource DaisyPrimaryContentBrush}">
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Full Width Navbar -->
                    <TextBlock Text="Full Width Navbar" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <StackPanel Spacing="6">
                            <TextBlock Text="No corner radius (IsFullWidth=True)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar IsFullWidth="True">
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarCenter>
                                    <controls:DaisyMenu Orientation="Horizontal">
                                        <ListBoxItem>Home</ListBoxItem>
                                        <ListBoxItem>About</ListBoxItem>
                                        <ListBoxItem>Contact</ListBoxItem>
                                    </controls:DaisyMenu>
                                </controls:DaisyNavbar.NavbarCenter>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <controls:DaisyButton Variant="Primary" Content="Sign In"/>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Navbar without Shadow -->
                    <TextBlock Text="Navbar without Shadow" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <StackPanel Spacing="6">
                            <TextBlock Text="HasShadow=False" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar HasShadow="False" Background="{DynamicResource DaisyBase200Brush}">
                                <controls:DaisyNavbar.NavbarStart>
                                    <controls:DaisyButton Variant="Ghost" FontSize="20" FontWeight="Bold" Content="Flowery"/>
                                </controls:DaisyNavbar.NavbarStart>
                                <controls:DaisyNavbar.NavbarEnd>
                                    <controls:DaisyButton ButtonStyle="Outline" Content="Login"/>
                                </controls:DaisyNavbar.NavbarEnd>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>

                    <!-- Simple Content Fallback -->
                    <TextBlock Text="Simple Content (Fallback)" FontWeight="SemiBold" Margin="0,16,0,0"/>
                    <StackPanel Spacing="16">
                        <StackPanel Spacing="6">
                            <TextBlock Text="Using Content property directly (legacy support)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyNavbar>
                                <StackPanel Orientation="Horizontal" Spacing="10">
                                    <TextBlock Text="Simple Navbar" FontWeight="Bold" FontSize="18" VerticalAlignment="Center"/>
                                    <controls:DaisyButton Variant="Ghost" Content="Link 1"/>
                                    <controls:DaisyButton Variant="Ghost" Content="Link 2"/>
                                </StackPanel>
                            </controls:DaisyNavbar>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>

                <controls:DaisyDivider />

                <!-- Pagination -->
                <StackPanel Spacing="12">
                    <local:SectionHeader SectionId="pagination" Title="Pagination" />
                    <WrapPanel Orientation="Horizontal">
                        <!-- Basic with active button -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="With Active Button" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyPagination CurrentPage="2">
                                <controls:DaisyPaginationItem Content="1" PageNumber="1" />
                                <controls:DaisyPaginationItem Content="2" PageNumber="2" />
                                <controls:DaisyPaginationItem Content="3" PageNumber="3" />
                                <controls:DaisyPaginationItem Content="4" PageNumber="4" />
                            </controls:DaisyPagination>
                        </StackPanel>

                        <!-- With disabled button (ellipsis) -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="With Disabled Button" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyPagination>
                                <controls:DaisyPaginationItem Content="1" PageNumber="1" />
                                <controls:DaisyPaginationItem Content="2" PageNumber="2" />
                                <controls:DaisyPaginationItem Content="..." IsEnabled="False" />
                                <controls:DaisyPaginationItem Content="99" PageNumber="99" />
                                <controls:DaisyPaginationItem Content="100" PageNumber="100" />
                            </controls:DaisyPagination>
                        </StackPanel>

                        <!-- Prev/Next style -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Prev/Next Buttons" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyPagination>
                                <controls:DaisyPaginationItem Content="«" />
                                <controls:DaisyPaginationItem Content="Page 22" />
                                <controls:DaisyPaginationItem Content="»" />
                            </controls:DaisyPagination>
                        </StackPanel>

                        <!-- Outline style -->
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Outline Style" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyPagination ButtonStyle="Outline">
                                <controls:DaisyPaginationItem Content="Previous page" />
                                <controls:DaisyPaginationItem Content="Next" />
                            </controls:DaisyPagination>
                        </StackPanel>
                    </WrapPanel>

                    <!-- Sizes -->
                    <TextBlock Text="Sizes" FontSize="12" Opacity="0.7" Margin="0,12,0,0"/>
                    <StackPanel Spacing="12">
                        <controls:DaisyPagination Size="ExtraSmall" CurrentPage="2">
                            <controls:DaisyPaginationItem Content="1" PageNumber="1" />
                            <controls:DaisyPaginationItem Content="2" PageNumber="2" />
                            <controls:DaisyPaginationItem Content="3" PageNumber="3" />
                            <controls:DaisyPaginationItem Content="4" PageNumber="4" />
                        </controls:DaisyPagination>
                        <controls:DaisyPagination Size="Small" CurrentPage="2">
                            <controls:DaisyPaginationItem Content="1" PageNumber="1" />
                            <controls:DaisyPaginationItem Content="2" PageNumber="2" />
                            <controls:DaisyPaginationItem Content="3" PageNumber="3" />
                            <controls:DaisyPaginationItem Content="4" PageNumber="4" />
                        </controls:DaisyPagination>
                        <controls:DaisyPagination Size="Medium" CurrentPage="2">
                            <controls:DaisyPaginationItem Content="1" PageNumber="1" />
                            <controls:DaisyPaginationItem Content="2" PageNumber="2" />
                            <controls:DaisyPaginationItem Content="3" PageNumber="3" />
                            <controls:DaisyPaginationItem Content="4" PageNumber="4" />
                        </controls:DaisyPagination>
                        <controls:DaisyPagination Size="Large" CurrentPage="2">
                            <controls:DaisyPaginationItem Content="1" PageNumber="1" />
                            <controls:DaisyPaginationItem Content="2" PageNumber="2" />
                            <controls:DaisyPaginationItem Content="3" PageNumber="3" />
                            <controls:DaisyPaginationItem Content="4" PageNumber="4" />
                        </controls:DaisyPagination>
                        <controls:DaisyPagination Size="ExtraLarge" CurrentPage="2">
                            <controls:DaisyPaginationItem Content="1" PageNumber="1" />
                            <controls:DaisyPaginationItem Content="2" PageNumber="2" />
                            <controls:DaisyPaginationItem Content="3" PageNumber="3" />
                            <controls:DaisyPaginationItem Content="4" PageNumber="4" />
                        </controls:DaisyPagination>
                    </StackPanel>
                </StackPanel>

                <controls:DaisyDivider />

                <!-- Dock -->
                <StackPanel Spacing="12">
                    <local:SectionHeader SectionId="dock" Title="Dock" />
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Default Dock" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyDock ItemSelected="OnDockItemSelected">
                                <Button Tag="Home">
                                    <StackPanel>
                                        <PathIcon Data="{StaticResource DaisyIconHome}" />
                                        <TextBlock Classes="dock-label" Text="Home" />
                                    </StackPanel>
                                </Button>
                                <Button Tag="Files" Classes="dock-active">
                                    <StackPanel>
                                        <PathIcon Data="{StaticResource DaisyIconDocument}" />
                                        <TextBlock Classes="dock-label" Text="Files" />
                                    </StackPanel>
                                </Button>
                                <Button Tag="Settings">
                                    <StackPanel>
                                        <PathIcon Data="{StaticResource DaisyIconSettings}" />
                                        <TextBlock Classes="dock-label" Text="Settings" />
                                    </StackPanel>
                                </Button>
                            </controls:DaisyDock>
                        </StackPanel>

                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Small Dock" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyDock Size="Small" ItemSelected="OnDockItemSelected">
                                <Button Tag="Home">
                                    <StackPanel>
                                        <PathIcon Data="{StaticResource DaisyIconHome}" />
                                        <TextBlock Classes="dock-label" Text="Home" />
                                    </StackPanel>
                                </Button>
                                <Button Tag="Files" Classes="dock-active">
                                    <StackPanel>
                                        <PathIcon Data="{StaticResource DaisyIconDocument}" />
                                        <TextBlock Classes="dock-label" Text="Files" />
                                    </StackPanel>
                                </Button>
                            </controls:DaisyDock>
                        </StackPanel>

                        <StackPanel Spacing="10" Margin="0,0,40,20">
                            <TextBlock Text="Extra Small Dock (Icons only)" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyDock Size="ExtraSmall" ItemSelected="OnDockItemSelected">
                                <Button Tag="Home">
                                    <PathIcon Data="{StaticResource DaisyIconHome}" />
                                </Button>
                                <Button Classes="dock-active" Tag="Files">
                                    <PathIcon Data="{StaticResource DaisyIconDocument}" />
                                </Button>
                                <Button Tag="Settings">
                                    <PathIcon Data="{StaticResource DaisyIconSettings}" />
                                </Button>
                            </controls:DaisyDock>
                        </StackPanel>
                    </WrapPanel>
                </StackPanel>

                <controls:DaisyDivider />

                <!-- Steps -->
                <StackPanel Spacing="12">
                    <local:SectionHeader SectionId="steps" Title="Steps" />
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,40,20" Width="300">
                            <controls:DaisySteps SelectedIndex="1">
                                <TextBlock Text="Register" />
                                <TextBlock Text="Plan" />
                                <TextBlock Text="Purchase" />
                            </controls:DaisySteps>
                        </StackPanel>
                    </WrapPanel>
                </StackPanel>

                <controls:DaisyDivider />

                <!-- Tabs -->
                <StackPanel Spacing="12">
                    <local:SectionHeader SectionId="tabs" Title="Tabs" />
                    <WrapPanel Orientation="Horizontal">
                        <StackPanel Spacing="10" Margin="0,0,20,10">
                            <TextBlock Text="None" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyTabs Variant="None">
                                <TabItem Header="Tab 1">
                                    <TextBlock Text="Content 1" Padding="10"/>
                                </TabItem>
                                <TabItem Header="Tab 2">
                                    <TextBlock Text="Content 2" Padding="10"/>
                                </TabItem>
                            </controls:DaisyTabs>
                        </StackPanel>

                        <StackPanel Spacing="10" Margin="0,0,20,10">
                            <TextBlock Text="Bordered" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyTabs Variant="Bordered">
                                <TabItem Header="Tab 1">
                                    <TextBlock Text="Content 1" Padding="10"/>
                                </TabItem>
                                <TabItem Header="Tab 2">
                                    <TextBlock Text="Content 2" Padding="10"/>
                                </TabItem>
                            </controls:DaisyTabs>
                        </StackPanel>

                        <StackPanel Spacing="10" Margin="0,0,20,10">
                            <TextBlock Text="Lifted" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyTabs Variant="Lifted">
                                <TabItem Header="Tab 1">
                                    <TextBlock Text="Content 1" Padding="10"/>
                                </TabItem>
                                <TabItem Header="Tab 2">
                                    <TextBlock Text="Content 2" Padding="10"/>
                                </TabItem>
                            </controls:DaisyTabs>
                        </StackPanel>

                        <StackPanel Spacing="10" Margin="0,0,20,10">
                            <TextBlock Text="Boxed" FontSize="12" Opacity="0.7"/>
                            <controls:DaisyTabs Variant="Boxed">
                                <TabItem Header="Tab 1">
                                    <TextBlock Text="Content 1" Padding="10"/>
                                </TabItem>
                                <TabItem Header="Tab 2">
                                    <TextBlock Text="Content 2" Padding="10"/>
                                </TabItem>
                            </controls:DaisyTabs>
                        </StackPanel>
                    </WrapPanel>
                </StackPanel>

            </StackPanel>
        </ScrollViewer>

        <!-- Toast Container -->
        <controls:DaisyToast x:Name="NavigationToast" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,20" />
    </Grid>

</UserControl>
