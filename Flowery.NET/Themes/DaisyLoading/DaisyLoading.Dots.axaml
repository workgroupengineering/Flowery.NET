    <!-- ==================== COUNTDOWN SPINNER ==================== -->
    <!-- CountdownSpinner - 12 dots toggle like clock ticking -->
    <Style Selector="controls|DaisyLoading[Variant=CountdownSpinner]">
        <Setter Property="Template">
            <ControlTemplate>
                <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Stretch="Uniform">
                    <Canvas Width="24" Height="24">
                        <!-- 12 dots arranged in a circle (clock positions) -->
                        <Ellipse Classes="cd1" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="10.75" Canvas.Top="1"/>
                        <Ellipse Classes="cd2" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="15.5" Canvas.Top="2.5"/>
                        <Ellipse Classes="cd3" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="19" Canvas.Top="6"/>
