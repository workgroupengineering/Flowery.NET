                        <Ellipse Classes="cd7" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="10.75" Canvas.Top="20.5"/>
                        <Ellipse Classes="cd8" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="6" Canvas.Top="19"/>
                        <Ellipse Classes="cd9" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="2.5" Canvas.Top="15.5"/>
                        <Ellipse Classes="cd10" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="1" Canvas.Top="10.75"/>
                        <Ellipse Classes="cd11" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="2.5" Canvas.Top="6"/>
                        <Ellipse Classes="cd12" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="6" Canvas.Top="2.5"/>
                    </Canvas>
                </Viewbox>
            </ControlTemplate>
        </Setter>
    </Style>

