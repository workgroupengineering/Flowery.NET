    <!-- ==================== RIPPLE MATRIX ==================== -->
    <!-- RippleMatrix - brightness ripples outward from center -->
    <Style Selector="controls|DaisyLoading[Variant=RippleMatrix]">
        <Setter Property="Template">
            <ControlTemplate>
                <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Stretch="Uniform">
                    <Canvas Width="24" Height="24">
                        <!-- 3x3 grid of dots -->
                        <!-- Center -->
                        <Ellipse Classes="ripple-c" Width="4" Height="4" Fill="{TemplateBinding Foreground}" Canvas.Left="10" Canvas.Top="10"/>
                        <!-- Ring 1 (adjacent) -->
                        <Ellipse Classes="ripple-r1" Width="3" Height="3" Fill="{TemplateBinding Foreground}" Canvas.Left="10.5" Canvas.Top="3"/>
                        <Ellipse Classes="ripple-r1" Width="3" Height="3" Fill="{TemplateBinding Foreground}" Canvas.Left="10.5" Canvas.Top="18"/>
                        <Ellipse Classes="ripple-r1" Width="3" Height="3" Fill="{TemplateBinding Foreground}" Canvas.Left="3" Canvas.Top="10.5"/>
                        <Ellipse Classes="ripple-r1" Width="3" Height="3" Fill="{TemplateBinding Foreground}" Canvas.Left="18" Canvas.Top="10.5"/>
                        <!-- Ring 2 (corners) -->
                        <Ellipse Classes="ripple-r2" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="3" Canvas.Top="3"/>
                        <Ellipse Classes="ripple-r2" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="18.5" Canvas.Top="3"/>
                        <Ellipse Classes="ripple-r2" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="3" Canvas.Top="18.5"/>
                        <Ellipse Classes="ripple-r2" Width="2.5" Height="2.5" Fill="{TemplateBinding Foreground}" Canvas.Left="18.5" Canvas.Top="18.5"/>
                    </Canvas>
                </Viewbox>
            </ControlTemplate>
        </Setter>
    </Style>

    <Style Selector="controls|DaisyLoading[Variant=RippleMatrix] /template/ Ellipse.ripple-c">
        <Style.Animations>
            <Animation Duration="0:0:1.2" IterationCount="Infinite">
                <KeyFrame KeyTime="0:0:0"><Setter Property="Opacity" Value="1"/></KeyFrame>
                <KeyFrame KeyTime="0:0:0.2"><Setter Property="Opacity" Value="0.3"/></KeyFrame>
                <KeyFrame KeyTime="0:0:1"><Setter Property="Opacity" Value="0.3"/></KeyFrame>
